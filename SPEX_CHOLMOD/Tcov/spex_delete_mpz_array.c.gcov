        -:   10:
        -:   11:/* Purpose: This function clears the memory used for an mpz vector of size n. 
        -:   12: * Call this function for all mpz vectors when done.
        -:   13: * 
        -:   14: * Input is a mpz_t** array which is destroyed upon function completion
        -:   15: * 
        -:   16: */
        -:   17:
        -:   18:#include "spex_internal.h"
        -:   19:
    #####:   20:void spex_delete_mpz_array
        -:   21:(
        -:   22:    mpz_t **x,      // mpz array to be deleted
        -:   23:    int64_t n       // Size of x
        -:   24:)
        -:   25:{
    #####:   26:    if (x == NULL || (*x) == NULL) {return;}
    #####:   27:    for (int64_t i = 0; i < n; i++)
        -:   28:    {
    #####:   29:        if ( (*x) [i] != NULL)
        -:   30:        {
    #####:   31:            SPEX_MPZ_CLEAR((*x) [i]);
        -:   32:        }
        -:   33:    }
    #####:   34:    SPEX_FREE ((*x));
        -:   35:}
