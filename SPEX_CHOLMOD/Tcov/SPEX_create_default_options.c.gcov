        -:   15:#include "spex_internal.h"
        -:   16:
    #####:   17:SPEX_options* SPEX_create_default_options ( void )
        -:   18:{
        -:   19:
    #####:   20:    if (!spex_initialized ( )) return (NULL) ;
        -:   21:
        -:   22:    //--------------------------------------------------------------------------
        -:   23:    // allocate the option struct
        -:   24:    //--------------------------------------------------------------------------
        -:   25:
    #####:   26:    SPEX_options* option = SPEX_malloc(sizeof(SPEX_options)) ;
    #####:   27:    if (!option)
        -:   28:    {
